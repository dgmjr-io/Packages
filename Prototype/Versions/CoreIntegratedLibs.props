<Project>
    <PropertyGroup>
        <CoreIntegratedLibsVersion Condition="$(TargetFramework.CompareTo('net6')) >= 0">Unsupported</CoreIntegratedLibsVersion>
        <CoreIntegratedLibsVersion Condition="$(TargetFramework.CompareTo('net7')) >= 0">Unsupported</CoreIntegratedLibsVersion>
        <CoreIntegratedLibsVersion Condition="$(TargetFramework.CompareTo('net8')) >= 0">Unsupported</CoreIntegratedLibsVersion>
        <CoreIntegratedLibsVersion Condition="$(TargetFramework.StartsWith('nststandard'))">4.3.0</CoreIntegratedLibsVersion>
    </PropertyGroup>
</Project>
