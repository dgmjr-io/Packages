<Project>
  <PropertyGroup Label="AutoMapper">
    <AutoMapperVersion Condition="!$(DefineConstants.Contains('NETSTANDARD1_1_OR_GREATER'))">Unsupported</AutoMapperVersion>
    <AutoMapperVersion Condition="$(DefineConstants.Contains('NETSTANDARD1_1_OR_GREATER'))">[6.2.2,)</AutoMapperVersion>
    <AutoMapperVersion Condition="$(DefineConstants.Contains('NETSTANDARD1_3_OR_GREATER'))">[7.0.1,)</AutoMapperVersion>
    <AutoMapperVersion Condition="$(DefineConstants.Contains('NETSTANDARD2_0_OR_GREATER'))">[10.1.1,)</AutoMapperVersion>
    <AutoMapperVersion Condition="$(DefineConstants.Contains('NETSTANDARD2_1_OR_GREATER'))">[12.0.1,)</AutoMapperVersion>

    <AutoMapperDIVersion Condition="!$(DefineConstants.Contains('NETSTANDARD1_1_OR_GREATER'))">Unsupported</AutoMapperDIVersion>
    <AutoMapperDIVersion Condition="$(DefineConstants.Contains('NETSTANDARD1_1_OR_GREATER'))">[2.0.1,)</AutoMapperDIVersion>
    <AutoMapperDIVersion Condition="$(DefineConstants.Contains('NETSTANDARD2_0_OR_GREATER'))">[8.1.1,)</AutoMapperDIVersion>
    <AutoMapperDIVersion Condition="$(DefineConstants.Contains('NETSTANDARD2_1_OR_GREATER'))">[12.0.1,)</AutoMapperDIVersion>

    <AutoMapperExpresionMappingVersion Condition="!$(DefineConstants.Contains('NETSTANDARD2_0_OR_GREATER'))">Unsupported</AutoMapperExpresionMappingVersion>
    <AutoMapperExpresionMappingVersion Condition="$(DefineConstants.Contains('NETSTANDARD2_0_OR_GREATER'))">[4.1.5,)</AutoMapperExpresionMappingVersion>
    <AutoMapperExpresionMappingVersion Condition="$(DefineConstants.Contains('NETSTANDARD2_1_OR_GREATER'))">[6.0.4,)</AutoMapperExpresionMappingVersion>
    <AutoMapperExpresionMappingVersion Condition="$(DefineConstants.Contains('NET6_0_OR_GREATER'))">[7.0.7,)</AutoMapperExpresionMappingVersion>

    <AutoMapperEnumMappingVersion Condition="!$(DefineConstants.Contains('NETSTANDARD2_0_OR_GREATER'))">Unsupported</AutoMapperEnumMappingVersion>
    <AutoMapperEnumMappingVersion Condition="$(DefineConstants.Contains('NETSTANDARD2_0_OR_GREATER'))">[1.1.0,)</AutoMapperEnumMappingVersion>
    <AutoMapperEnumMappingVersion Condition="$(DefineConstants.Contains('NETSTANDARD2_1_OR_GREATER'))">[3.1.0,)</AutoMapperEnumMappingVersion>
    <AutoMapperEnumMappingVersion Condition="$(DefineConstants.Contains('NET6_0_OR_GREATER'))">[3.2.0,)</AutoMapperEnumMappingVersion>
  </PropertyGroup>
</Project>
